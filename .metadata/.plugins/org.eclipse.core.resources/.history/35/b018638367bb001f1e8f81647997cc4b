package teamTask01;

public abstract class UserVO {
	private String id;
	private String pw;
	private String name;
	private String age;
	
	public UserVO(String id, String pw, String name, String age) {
		super();
		id = this.id;
		pw = this.pw;
		name = this.name;
		age = this.age;
	}
	
	public String getId() {
		return id;
	}


	public String getPw() {
		return pw;
	}


	public String getName() {
		return name;
	}


	public String getAge() {
		return age;
	}


	abstract boolean checkID(String id);
}
