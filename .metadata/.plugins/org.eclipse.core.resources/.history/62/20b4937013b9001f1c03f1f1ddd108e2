package listTest;

import java.util.ArrayList;

public class ArrayListTest {
	public static void main(String[] args) {
		//  ArrayList 객체 생성
		ArrayList al = new ArrayList();
		System.out.println(al);
		System.out.println(al.toString());
		
		// 요소 추가 메소드
		al.add(1);
		al.add(2);
		System.out.println(al);
	}
}
