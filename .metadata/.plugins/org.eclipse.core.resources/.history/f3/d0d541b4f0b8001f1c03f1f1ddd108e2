package objextTest;

public class User {

	// 필드
	int userNumber;
	String nserName;
	
	// 기본 생성자
	public User() {
		super();
	}
	
	// 생성자 오버로딩
	public User(int userNumber, String nserName) {
		super();
		this.userNumber = userNumber;
		this.nserName = nserName;
	}

	// toString 오버라이딩
	@Override
	public String toString() {
		return "User [userNumber = " + userNumber + ", userName = " + userName + "]";
	}
	
	
}
