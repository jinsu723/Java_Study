package task;

public class Main {
	public static void main(String[] args) {
		MultipleInter mi1 = (num1, num2) -> num1 * num2;
		MultipleInter mi2 = (num1, num2) -> 0;
		
	}
	
	static MultipleInter calculator(boolean isTrue) {
		if(isTrue) {
			return mi1 = (num1, num2) -> num1 * num2;
		} else {
			return mi2 = (num1, num2) -> 0;
		}
	}
}
