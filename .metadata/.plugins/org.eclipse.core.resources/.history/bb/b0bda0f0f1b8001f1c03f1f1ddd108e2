package objextTest;

public class User {

	// 필드
	int userNumber;
	String userName;
	
	// 기본 생성자
	public User() {
		super();
	}
	
	// 생성자 오버로딩
	public User(int userNumber, String userName) {
		super();
		this.userNumber = userNumber;
		this.userName = userName;
	}

	// toString 오버라이딩
	@Override
	public String toString() {
		return "User [userNumber = " + userNumber + ", userName = " + userName + "]";
	}

	// hashCode 오버라이딩
	@Override
	public int hashCode() {
		return this.userNumber;
	}
	
	
	
	
}
