package innerTest;

// 1번 :  내부 클래스

public class Outer {
	// Outer 필드
	int var1;
	String data1 = "Outer";
	
	// Outer 클래스의 기본 생성자
	public Outer() {
		System.out.println("외부 클래스 생성자");
	}
	
	// 내부 클래스  Inner 정의
	class Inner{
		// 필드, 생성자, 메소드
		// Inner 클래스의 필드
		int var2 = 10;
		String data2 = "Inner";
		
		public Inner() {
//			super();
			System.out.println("내부 클래스 생성자");
		}
		
	}
	
	// 메소드
}
