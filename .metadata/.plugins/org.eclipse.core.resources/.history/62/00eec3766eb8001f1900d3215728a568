package task01;

public class Method {
	int sumNum(int num) {
		int count = 0;
		for(int i=0; i<=num; i++) {
			count += i;
		}
		return count;
	}
	
	boolean checkNum(String num) {
		for(int i=0; i<num.length(); i++) {
			if(num.charAt(i) > 57 || num.charAt(i) < 48) {
				return false;
			}
		}
		return true;
	}
	
}
