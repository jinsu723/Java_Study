package task;

public class Main {
	public static void main(String[] args) {
		calculator(true);
		calculator(false);
	}
	
	static MultipleInter calculator(boolean isTrue) {
		if(isTrue) {
			MultipleInter mi1 = (num1, num2) -> num1 * num2;
			return mi1;
		} else {
			MultipleInter mi2 = (num1, num2) -> 0;
			return mi2;
		}
	}
}
