package teamTask01;

public abstract class UserVO {
	private String id;
	private String pw;
	private String name;
	private String age;
	
	public UserVO(String iD, String pW, String name, String age) {
		super();
		id = this.id;
		pw = this.pw;
		name = this.name;
		age = this.age;
	}
	
	abstract boolean checkID(String ID);
}
