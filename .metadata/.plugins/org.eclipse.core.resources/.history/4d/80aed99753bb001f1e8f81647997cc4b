package mapTest;

import java.util.HashMap;

// 1ë²ˆ : hashMap

public class HashMapTest01 {
	public static void main(String[] args) {
		HashMap<String, Integer> map = new HashMap<>();
		System.out.println(map);
		System.out.println(map.toString());
		
		map.put("A", 1);
		System.out.println(map);
	}
}
